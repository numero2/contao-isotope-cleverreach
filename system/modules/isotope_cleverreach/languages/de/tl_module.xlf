      </trans-unit>

      <trans-unit id="tl_module.iso_cleverReachGroup.0">
        <source>CleverReach Group</source>
        <target>CleverReach Empfängergruppe</target>
      </trans-unit>
      <trans-unit id="tl_module.iso_cleverReachGroup.1">
